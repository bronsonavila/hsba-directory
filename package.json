{
  "name": "hsba-membership-data",
  "version": "1.0.0",
  "description": "A Node application for processing data from the Hawaii State Bar Association's `Membership Directory`.",
  "main": "index.js",
  "scripts": {
    "get_admitted_hi_bar_alphabetical": "node -e 'require(\"./index.js\").getGroup(\"admitted_hi_bar\")'",
    "get_admitted_hi_bar_count": "node -e 'require(\"./index.js\").getGroup(\"admitted_hi_bar\", \"count\")'",
    "get_email_alphabetical": "node -e 'require(\"./index.js\").getGroup(\"email\")'",
    "get_email_count": "node -e 'require(\"./index.js\").getGroup(\"email\", \"count\")'",
    "get_employer_alphabetical": "node -e 'require(\"./index.js\").getGroup(\"employer\")'",
    "get_employer_count": "node -e 'require(\"./index.js\").getGroup(\"employer\", \"count\")'",
    "get_law_school_alphabetical": "node -e 'require(\"./index.js\").getGroup(\"law_school\")'",
    "get_law_school_count": "node -e 'require(\"./index.js\").getGroup(\"law_school\", \"count\")'",
    "get_license_type_alphabetical": "node -e 'require(\"./index.js\").getGroup(\"license_type\")'",
    "get_license_type_count": "node -e 'require(\"./index.js\").getGroup(\"license_type\", \"count\")'",
    "get_status_alphabetical": "node -e 'require(\"./index.js\").getGroup(\"status\")'",
    "get_status_count": "node -e 'require(\"./index.js\").getGroup(\"status\", \"count\")'",
    "write_full_members": "node -e 'require(\"./index.js\").writeFullMemberResultsToJson()'"
  },
  "author": "Bronson Avila",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.3",
    "d3": "^5.16.0",
    "d3-array": "^2.4.0"
  }
}
